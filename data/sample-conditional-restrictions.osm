  <!-- WAYS WITH CONDITIONAL RESTRICTIONS -->
  
  <!-- 1. Time-based access restriction (city center pedestrian zone) -->
  <way id='-100' version='1'>
    <nd ref='-1'/>
    <nd ref='-2'/>
    <tag k='highway' v='pedestrian'/>
    <tag k='name' v='Pedestrian Zone Main Street'/>
    <tag k='access' v='yes'/>
    <tag k='motor_vehicle:conditional' v='no @ (Mo-Sa 08:00-20:00)'/>
    <tag k='note' v='Motor vehicles prohibited during shopping hours'/>
  </way>
  
